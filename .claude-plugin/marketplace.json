{
  "name": "claudekit",
  "owner": {
    "name": "Aotokitsuruya"
  },
  "metadata": {
    "description": "A collection of plugins for Claude Code used by Aotokitsuruya."
  },
  "plugins": [
    {
      "name": "autonoe",
      "source": {
        "source": "url",
        "url": "https://github.com/elct9620/autonoe-skill.git"
      }
    },
    {
      "name": "dependabot",
      "source": "./plugins/dependabot",
      "description": "Commands to interact with Dependabot on GitHub repositories."
    },
    {
      "name": "git",
      "source": "./plugins/git",
      "description": "Common repository operations for git"
    },
    {
      "name": "license",
      "source": "./plugins/license",
      "description": "Commands to manage open source licenses in current repository"
    },
    {
      "name": "rubric",
      "source": "./plugins/rubric",
      "description": "Automated code standards checks based on custom rubrics"
    },
    {
      "name": "spec",
      "source": {
        "source": "url",
        "url": "https://github.com/elct9620/claude-spec-plugin.git"
      }
    },
    {
      "name": "hono",
      "source": {
        "source": "url",
        "url": "https://github.com/elct9620/claude-hono-plugin.git"
      }
    },
    {
      "name": "ruby",
      "source": {
        "source": "url",
        "url": "https://github.com/elct9620/claude-ruby-plugin.git"
      }
    },
    {
      "name": "coding",
      "source": {
        "source": "url",
        "url": "https://github.com/elct9620/ai-coding-skills.git"
      }
    },
    {
      "name": "gha",
      "source": "./plugins/gha",
      "description": "Commands to create and manage GitHub Actions workflows."
    }
  ]
}
